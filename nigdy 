#include <iostream>
using namespace std;

int main()
{
    int a[3][3];
    int b[3][5];

    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
        {
            cout << "Podaj element tablicy a[" << i << "][" << j << "] = ";
            cin >> a[i][j];
        }

    for(int i=0;i<3;i++)
        for(int j=0;j<5;j++)
        {
            if(j < 3)
                b[i][j] = a[i][j];
            else
                b[i][j] = a[i][j-3];
        }

    cout << "Pelna macierz ze skopiowanymi elementami wyglada nastepujaco:" << endl;
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<5;j++)
            cout << b[i][j] << " ";
        cout << endl;
    }

    int suma1 = 0, suma2 = 0;

    for(int i=0;i<3;i++)
        suma1 += b[0][i] * b[1][i+1] * b[2][i+2];

    for(int i=0;i<3;i++)
        suma2 += b[0][i+2] * b[1][i+1] * b[2][i];

    cout << "Wyznacznik tablicy a = " << suma1 - suma2 << endl;

    return 0;
}